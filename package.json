{
  "name": "streamvault",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "deploy": "nuxt build && wrangler pages deploy dist --d1 DB=$D1_DATABASE_ID",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "worker:dev": "wrangler dev worker/src/index.ts",
    "worker:deploy": "wrangler deploy worker/src/index.ts",
    "db:migrate": "wrangler d1 execute streamvault-db --local --file=server/database/migrations.sql",
    "db:migrate:prod": "wrangler d1 execute streamvault-db --remote --file=server/database/migrations.sql",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.90",
    "@nuxt/ui": "^3.0.0",
    "@vueuse/core": "^14.2.1",
    "@vueuse/nuxt": "^14.2.1",
    "hls.js": "^1.6.15",
    "hono": "^4.11.9",
    "nuxt": "^3.16.0",
    "qrcode": "^1.5.4",
    "tailwindcss": "^4.1.18",
    "uuid": "^11.1.0",
    "vue": "^3.5.28",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "@nuxt/fonts": "^0.14.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "typescript": "^5.9.3",
    "wrangler": "^4.65.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
