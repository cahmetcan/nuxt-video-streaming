{
  "name": "streamvault",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "worker:dev": "wrangler dev worker/src/index.ts",
    "worker:deploy": "wrangler deploy worker/src/index.ts",
    "db:migrate": "wrangler d1 execute streamvault-db --local --file=server/database/migrations.sql",
    "db:migrate:prod": "wrangler d1 execute streamvault-db --file=server/database/migrations.sql",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@iconify-json/lucide": "^1.2.8",
    "@nuxt/ui": "^3.0.0",
    "@vueuse/core": "^11.3.0",
    "@vueuse/nuxt": "^11.3.0",
    "hls.js": "^1.5.17",
    "hono": "^4.6.0",
    "nuxt": "^3.14.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0",
    "qrcode": "^1.5.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxtjs/color-mode": "^3.5.2",
    "typescript": "^5.6.0",
    "wrangler": "^3.91.0"
  }
}
